{
    "name": "we-do-code-ltd/test-suite-bundle",
    "type": "symfony-bundle",
    "description": "Provides a common automated test suite with filter based on attributes",
    "keywords": [],
    "homepage": "https://www.wedocode.co.uk",
    "license": "MIT",
    "authors": [
        {
            "name": "Tamas Dobo",
            "email": "tamas@wedocode.co.uk"
        },
        {
            "name": "We Do Code Ltd",
            "homepage": "https://www.wedocode.co.uk"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "infection/infection": "^0.21",
        "johnkary/phpunit-speedtrap": "^4.0-dev",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-mockery": "^0.12.13",
        "phpstan/phpstan-phpunit": "^0.12.18",
        "phpstan/phpstan-symfony": "^0.12.21",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/dependency-injection": "^5.2",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/finder": "^5.2",
        "symfony/serializer": "^5.2"
    },
    "autoload": {
        "psr-4": { "WeDoCode\\Bundle\\WeDoCodeTestSuiteBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
